<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Library</title>
  </head>
  <body>
    <%- include('../partials/_navbar.ejs') %>
    <h1>All Books</h1>
    <a href="/books/new">Add a New Book</a>
    <ul>
      <% books.forEach((book) => { %>
      <li>
        <a href="/books/<%= book._id %>">
        <%= book.name %> owned by <%= book.owner.username %>.
      </li>
      <% }) %>
    </ul>
  </body>
</html>


